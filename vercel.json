{
  "functions": {
    "app/main.py": {
      "maxDuration": 60,
      "excludeFiles": "{tests/**,__tests__/**,**/*.test.py,**/test_*.py,chroma_db/**,data/**,demo_script.md,deployment_guide.md,**/*.md,Dockerfile,**/*.pyc,**/__pycache__/**}"
    }
